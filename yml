name: CI Windows - Compilaci贸 i Proves

on: [push, pull_request]

jobs:
  build:
    runs-on: windows-latest

    steps:
    - name: Clonar repositori
      uses: actions/checkout@v2

    - name: Instal路lar MinGW per compilar C
      run: choco install mingw --version=8.1.0

    - name: Verificar instal路laci贸
      run: gcc --version

    - name: Compilar el programa
      run: gcc -o github.exe mainGithub.c

    - name: Executar proves
      run: ./github.exe
